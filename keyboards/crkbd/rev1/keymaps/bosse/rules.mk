# Link Time Optimization
LTO_ENABLE = yes
# Emulate a mouse using keyboard
MOUSEKEY_ENABLE = yes
# Hit multiple keys at once and for different output
COMBO_ENABLE = yes
# OLED feature
OLED_ENABLE = yes
# Audio control and System control(+450)
EXTRAKEY_ENABLE = yes
# Nkey Rollover
NKRO_ENABLE = yes
# Override modifier key
KEY_OVERRIDE_ENABLE = no
# Support for one-handed typing without requiring a separate layer
SWAP_HANDS_ENABLE = no
# Enable configuring using VIA
VIA_ENABLE = no
# Game controller input
JOYSTICK_ENABLE = no
# Console for debug(+400)
CONSOLE_ENABLE = no
# Commands for debug and configuration
COMMAND_ENABLE = no
# Enable keyboard backlight functionality
BACKLIGHT_ENABLE = no
# MIDI controls
MIDI_ENABLE = no
# Audio output on port C6
AUDIO_ENABLE = no
# Unicode
UNICODE_ENABLE = no
# Enable Bluetooth with the Adafruit EZ-Key HID
BLUETOOTH_ENABLE = no
# Enable WS2812 RGB underlight.
RGBLIGHT_ENABLE = no
# Disabled on purpose to reduce firmware size
SPACE_CADET_ENABLE = no
GRAVE_ESC_ENABLE = no
MAGIC_ENABLE = no
